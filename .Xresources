! urxvt
URxvt*buffered: true
URxvt*cursorBlink: true
URxvt*underlineColor: #0D1117
URxvt*font:  xft:Iosevka Term:size=12:antialias=true
URxvt*depth: 32
URxvt*borderless: 3
URxvt*scrollBar: false
URxvt*loginShell: true
Urxvt*secondaryScroll:  true    # Enable Shift-PageUp/Down in screen
URxvt*saveLines: 5000
URxvt*termName: rxvt-unicode
URxvt.perl-ext-common: default,matcher
URxvt.internalBorder: 24 
URxvt.urlLauncher: Firefox
URxvt.matcher.button: 1
! X colors.
! Generated by 'wal'
*foreground:        #d4e0e1
*background:        #0d090a
*.foreground:       #d4e0e1
*.background:       #0d090a
emacs*foreground:   #d4e0e1
emacs*background:   #0d090a
URxvt*foreground:   #d4e0e1
XTerm*foreground:   #d4e0e1
UXTerm*foreground:  #d4e0e1
URxvt*background:   [100]#0d090a
XTerm*background:   #0d090a
UXTerm*background:  #0d090a
URxvt*cursorColor:  #d4e0e1
XTerm*cursorColor:  #d4e0e1
UXTerm*cursorColor: #d4e0e1
URxvt*borderColor:  [100]#0d090a

! Colors 0-15.
*.color0: #0d090a
*color0:  #0d090a
*.color1: #595B60
*color1:  #595B60
*.color2: #8D7465
*color2:  #8D7465
*.color3: #5A768B
*color3:  #5A768B
*.color4: #698BA3
*color4:  #698BA3
*.color5: #7BA9CC
*color5:  #7BA9CC
*.color6: #86B7DC
*color6:  #86B7DC
*.color7: #d4e0e1
*color7:  #d4e0e1
*.color8: #949c9d
*color8:  #949c9d
*.color9: #595B60
*color9:  #595B60
*.color10: #8D7465
*color10:  #8D7465
*.color11: #5A768B
*color11:  #5A768B
*.color12: #698BA3
*color12:  #698BA3
*.color13: #7BA9CC
