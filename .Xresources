! Black color that will not be affected by bold highlighting.
*.color66: #0d090a
*color66:  #0d090a

! Xclock colors.
XClock*foreground: #d4e0e1
XClock*background: #0d090a
XClock*majorColor:  rgba:d4/e0/e1/ff
XClock*minorColor:  rgba:d4/e0/e1/ff
XClock*hourColor:   rgba:d4/e0/e1/ff
XClock*minuteColor: rgba:d4/e0/e1/ff
XClock*secondColor: rgba:d4/e0/e1/ff

! Set depth to make transparency work.
URxvt*depth: 32

